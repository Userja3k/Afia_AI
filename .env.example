# Environment variables for AfiaAI Backend

# Host for the backend server
HOST=0.0.0.0

# Port for the backend server (Render sets this automatically)
PORT=8000

# Debug mode (True or False)
DEBUG=True

# Paths to ONNX models
MALARIA_MODEL_PATH=models/paludisme.onnx
SKIN_MODEL_PATH=models/skin.onnx

# Upload directory
UPLOAD_DIR=static/uploads

# CORS origins (comma separated)
CORS_ORIGINS=*
